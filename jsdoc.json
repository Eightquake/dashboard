{
  "plugins":[],
  "recurseDepth": 10,
  "source": {
    "include": [".", "README.md"],
    "exclude": ["node_modules", "modules/inactive"],
    "includePattern": ".+\\.js$",
    "excludePattern": ""
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "access": "all",
    "sort": false
  }
}
