{
  "tags": {
        "allowUnknownTags": ["category"]
  },
  "plugins":[
    "plugins/markdown",
    "node_modules/better-docs/category"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["."],
    "exclude": ["node_modules", "docs", "modules/inactive"],
    "includePattern": ".+\\.js$",
    "excludePattern": ""
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "better-docs": {
      "name": "Electron Dashboard by Victor Davidsson",
      "navigation": [
        {
        "label": "Github",
        "href": "https://github.com/Eightquake/dashboard"
        }
      ]
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/better-docs",
    "access": "all",
    "sort": false
  }
}
