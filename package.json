{
  "name": "dashboard-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "eslint": "^6.2.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "main": "src/js/main/main.js",
  "homepage": "./",
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "rescripts eject",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron ./src/js/main/main.js"
    "electron-dev": "cross-env NODE_ENV=development electron ./src/js/main/main.js",
    "electron-prod": "cross-env NODE_ENV=production electron ./src/js/main/main.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.11",
    "electron": "^6.0.3"
  }
}
