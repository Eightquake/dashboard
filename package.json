{
  "name": "dashboard",
  "version": "0.1.0",
  "description": "A simple Dashboard using Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron scripts/main/main.js",
    "gen-docs": "jsdoc -r -c jsdoc.json",
    "gen-css": "lessc ./resources/renderer/main.less ./resources/renderer/main.css && lessc ./resources/renderer/lighttheme.less ./resources/renderer/lighttheme.css && lessc ./resources/renderer/darktheme.less ./resources/renderer/darktheme.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eightquake/dashboard.git"
  },
  "author": "Victor Davidsson",
  "license": "GPL-3.0-or-later",
  "private": true,
  "bugs": {
    "url": "https://github.com/Eightquake/dashboard/issues",
    "email": "kontakt@victordavidsson.se"
  },
  "homepage": "https://github.com/Eightquake/dashboard#readme",
  "devDependencies": {
    "better-docs": "^1.1.6",
    "electron": "^5.0.6",
    "jsdoc": "^3.6.3",
    "less": "^3.9.0"
  },
  "dependencies": {
    "draggabilly": "^2.2.0",
    "events": "^3.0.0",
    "feedparser": "^2.2.9",
    "jquery": "^3.4.1",
    "jquery-bridget": "^2.0.1",
    "node-schedule": "^1.3.2",
    "packery": "^2.1.2",
    "path": "^0.12.7"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    }
  }
}
